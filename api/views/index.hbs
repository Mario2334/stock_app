
<div class="container">
  <table class="table table-dark">
    <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Company</th>
      <th scope="col">Price</th>
      <th scope="col">Volume</th>
      <th scope="col">YTD Change</th>
      <th scope="col">Volume * Price</th>
      <th scope="col">Total Employees</th>

    </tr>
    </thead>
    <tbody>
    {{#each data}}
        <tr id={{this._id}}>
          <th scope="row">{{@index}}</th>
          <td data-key="Company">{{this.Company}}</td>
          <td data-key="Price">{{this.Price}}</td>
          <td data-key="Volume">{{this.Volume}}</td>
            <td data-key="YTD Change">{{this.[YTD Change]}}</td>
            <td data-key="Volume * Price">{{this.[Volume * Price]}}</td>
            <td data-key="Total Employees">{{this.[Total Employees]}}</td>
        </tr>
    {{/each}}
    </tbody>
  </table>
</div>
